// Дана функция T(n) = 50n + 2000. Определить её асимптотический класс сложности. Написать программу для вычисления T(n) при заданном n и вывода итогового класса.
#include <iostream>
using namespace std;

int main() {
    int n;
    
    cout << "Введите n: ";
    cin >> n;

    int T_n = 50 * n + 2000;

    cout << "T(" << n << ") = " << T_n << endl;

    cout << "Асимптотический класс: O(n) линейное" << endl;

    return 0;
}
