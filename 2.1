#include <iostream>
#include <vector>
#include <chrono>

using namespace std;

double findMinEl1(vector<double> arr1) {
    double min1=arr1[0];
    for(int i=0;i<arr1.size();i++){
        if(arr1[i]<min1){
            min1=arr1[i];
        }
    }
   return min1;
}

